{
  "author": "Christian Stolz <hg8496@cstolz.de>",
  "name": "@hg8496/netmask",
  "description": "Parse, lookup and iterate IP network blocks",
  "version": "2.0.0",
  "homepage": "https://github.com/hg8496/node-netmask",
  "bugs": "https://github.com/hg8496/node-netmask/issues",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/hg8496/node-netmask.git"
  },
  "keywords": [
    "net",
    "mask",
    "ip",
    "network",
    "cidr",
    "netmask",
    "subnet",
    "ipcalc",
    "typescript"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A lib",
    "postversion": "git push && git push --tags",
    "build": "tsc",
    "format": "prettier --write \"lib/**/*.ts\" \"lib/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "test": "jest --coverage --config jestconfig.json",
    "coveralls": "jest --config jestconfig.json --coverage --coverageReporters=text-lcov | coveralls"
  },
  "engines": {
    "node": ">= 8.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@types/jest": "^24.0.11",
    "coveralls": "^3.0.3",
    "jest": "^24.7.1",
    "prettier": "^1.17.0",
    "ts-jest": "^24.0.2",
    "tslint": "^5.16.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.4.4"
  }
}
